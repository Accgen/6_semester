<Window x:Class="spp3.View.AddProvider"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:spp3.View"
        mc:Ignorable="d"
        Title="AddProvider" Height="230" Width="382">
    <Grid>
        <TextBlock Margin="9,34,289,62" Text="Name"/>
        <TextBox x:Name="Name" HorizontalAlignment="Left" Height="23" Margin="105,35,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="262" />
        <TextBlock Margin="9,92,289,4" Text="Country"/>
        <TextBox x:Name="Country" HorizontalAlignment="Left" Height="23" Margin="105,93,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="262" />
        <Button x:Name="insertBtn" Content="Add" HorizontalAlignment="Left" Margin="18,171,0,0" VerticalAlignment="Top" Width="75" Click="insertBtn_Click"/>
    </Grid>
</Window>
