<Window x:Class="spp3.View.AddClient"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:spp3.View"
        mc:Ignorable="d"
        Title="AddClient" Height="470" Width="422">
    <Grid>
        <TextBlock Margin="9,34,326,357" Text="Name"/>
        <TextBox x:Name="Name" HorizontalAlignment="Left" Height="23" Margin="105,35,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="262" />
        <TextBlock Margin="9,105,326,286" Text="Sourname"/>
        <TextBox x:Name="Sourname" HorizontalAlignment="Left" Height="23" Margin="105,106,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="262" />
        <TextBlock Margin="9,182,326,209" Text="Passwort"/>
        <TextBox x:Name="Passwort" HorizontalAlignment="Left" Height="23" Margin="105,183,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="262" />
        <Button x:Name="insertBtn" Content="Add" HorizontalAlignment="Left" Margin="21,261,0,0" VerticalAlignment="Top" Width="75" Click="insertBtn_Click"/>
    </Grid>
</Window>
